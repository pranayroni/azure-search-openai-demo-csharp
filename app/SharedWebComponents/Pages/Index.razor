@page "/"
<MudTimeline TimelineAlign="_timelineAlign">
    <MudTimelineItem  Elevation="25">
        <ItemOpposite>
            <MudText  Typo="Typo.h6">June 20th, 2024</MudText>
        </ItemOpposite>
        <ItemContent>
            <MudPaper Elevation="0" Class="mt-n1">
                <MudText  Typo="Typo.h6" GutterBottom="true">v2.0.0</MudText>
                <MudText Typo="Typo.body1">Migration of v1</MudText>
     <MudExpansionPanel Text="VIEW RELEASE NOTES" Expanded="false">
        <MudTreeView T="string" ReadOnly>
    <MudTreeViewItem Expanded='true' T='string' Text="Fix: Queries are independent of v1">
        <MudTreeViewItem Expanded='true' T='string' Text="Document Bot now queries from own resources instead of v1." />
    </MudTreeViewItem>
    <MudTreeViewItem Expanded='true' T='string' Text="Feature: Support over limited Knipper Documents">
        <MudTreeViewItem Expanded='true' T='string' Text="Only few Knipper documents have been uploaded to knowledgebase. Accuracy will improve as more are uploaded over course of development." />
    </MudTreeViewItem>
</MudTreeView>


    
    </MudExpansionPanel>
        
      
            </MudPaper>
        </ItemContent>
    </MudTimelineItem>
    <MudTimelineItem  Elevation="25" TimelineAlign="TimelineAlign.End">
        <ItemOpposite>
            <MudText  Typo="Typo.h6">June 25th, 2024</MudText>
        <MudAlert Severity="Severity.Warning">Authentication is temporarily disabled.</MudAlert>
        </ItemOpposite>
        <ItemContent>
            <MudPaper Elevation="0" Class="mt-n1">
                <MudText  Typo="Typo.h6" GutterBottom="true">v2.0.1</MudText>
                <MudText Typo="Typo.body2">Azure AD Authentication SSO Support</MudText>
                 <MudExpansionPanel Text="VIEW RELEASE NOTES" Expanded="false">
            <MudTreeView T="string" ReadOnly>
    <MudTreeViewItem Expanded='true' T='string' Text="Feature: Authentication Support">
        <MudTreeViewItem Expanded='true' T='string' Text="Microsoft SSO will authenticate the user with Knipper's Azure AD. User must be a member of the Knipper Azure Tenant to be authorized this service." />
    </MudTreeViewItem>
    <MudTreeViewItem Expanded='true' T='string' Text="Feature: Single filter support for query">
        <MudTreeViewItem Expanded='true' T='string' Text="Queries can now be filtered by a single tag using the 'Exclude Category' dropdown available in the chat view." />
    </MudTreeViewItem>
</MudTreeView>

        
                 
    </MudExpansionPanel>
            </MudPaper>
        </ItemContent>
    </MudTimelineItem>
    <MudTimelineItem Color="Color.Info" Elevation="25">
        <ItemOpposite>
            <MudText Color="Color.Info" Typo="Typo.h6" GutterBottom="true">July 1st, 2024</MudText>
        </ItemOpposite>
        <ItemContent>
            <MudPaper Elevation="0" Class="mt-n1">
                <MudText Color="Color.Info" Typo="Typo.h6" GutterBottom="true">v2.0.2</MudText>
                <MudText Typo="Typo.body2">AI Search Accuracy Improvements and Multi-Filter support</MudText>
                                 <MudExpansionPanel Text="VIEW RELEASE NOTES" Expanded="false">
           <MudTreeView T="string" ReadOnly>
    <MudTreeViewItem Expanded='true' T='string' Text="Enhancement: Improvements to search accuracy">
        <MudTreeViewItem Expanded='true' T='string' Text="Document Bot was not aware of all documents in knowledgebase pertaining to knipper." />
        <MudTreeViewItem Expanded='true' T='string' Text="Adding this context has improved accuracy significantly." />
    </MudTreeViewItem>
    <MudTreeViewItem Expanded='true' T='string' Text="Enhancement: Multi filter support for query">
        <MudTreeViewItem Expanded='true' T='string' Text="Queries can now be filtered by a single tag using the 'Exclude Category' dropdown available in the chat view." />
    </MudTreeViewItem>
</MudTreeView>
          
                 
    </MudExpansionPanel>
            </MudPaper>
        </ItemContent>
    </MudTimelineItem>
    <MudTimelineItem Color="Color.Warning" Elevation="25">
        <ItemOpposite>
            <MudText Color="Color.Warning" Typo="Typo.h6" GutterBottom="true">Development In Progress..</MudText>
        <MudAlert Severity="Severity.Success" Class='my-2'>Currently Active Version</MudAlert>
        <!-- <MudAlert Severity="Severity.Error" Class='my-2'>Currently querying from a new limited Knowledge Base. Responses may be inaccurate.</MudAlert> -->
        <MudAlert Severity="Severity.Normal" Variant="Variant.Filled" Class='my-2'>Release Scheduled: 7/12/24</MudAlert>

        </ItemOpposite>
        <ItemContent>
            <MudPaper Elevation="0" Class="mt-n1">
                <MudText Color="Color.Warning" Typo="Typo.h6" GutterBottom="true">v2.0.3-dev</MudText>
                <MudText Typo="Typo.body2">Major Database Refactor, Documents Upload/Update/Delete support, AI Search Improvements</MudText>
                                 <MudExpansionPanel Text="VIEW RELEASE PLAN" Expanded="false">
            <MudTreeView T="string" ReadOnly>
    <MudTreeViewItem T='string' Text="Enhancement: Migrating to new refactored vector database">
        <MudAlert Severity="Severity.Success" Dense="true" Class="my-2" Variant='Variant.Outlined'>Active</MudAlert>
        <MudTreeViewItem T='string' Class='my-4' Text="Updated vector database to allow embeddings to be associated with a file that can have multiple tags." />
        <MudTreeViewItem T='string' Class='my-4' Text="New blob database and vector databases are populated with existing files from document bot knowledge base." />
    </MudTreeViewItem>
    <MudTreeViewItem T='string' Text="Feature: Delete documents in knowledgebase">
        <MudAlert Severity="Severity.Success" Dense="true" Class="my-2" Variant='Variant.Outlined'>Active</MudAlert>
        <MudTreeViewItem T='string' Class = 'my-4' Text="Users can request to delete documents when viewing them. (Delete will remove the embeddings as well as the ENTIRE pdf from database)" />
        <MudTreeViewItem T='string' Class = 'my-4' Text="Ex. Requesting to delete 'foo-23.pdf' will remove ALL files prefixed 'foo' and their embeddings." />
    </MudTreeViewItem>
    <MudTreeViewItem T='string' Text="Feature: Update Documents support">
        <MudAlert Severity="Severity.Success" Dense="true" Class="my-2" Variant='Variant.Outlined'>Active</MudAlert>
        <MudTreeViewItem T='string' Class = 'my-4'Text="Uploading a file with existing filename will update it in the database and recompute embeddings." />
    </MudTreeViewItem>
    <MudTreeViewItem T='string' Text="Feature: Internet Search Defaulting">
        <MudAlert Severity="Severity.Normal" Dense="true" Class="my-2" Variant='Variant.Outlined'>Development In Progress</MudAlert>
        <MudTreeViewItem T='string' Class = 'my-4' Text="Document Bot will try to find answers on the internet, if the query is outside the scope of the knowledgebase." />
    </MudTreeViewItem>
</MudTreeView>


        <MudDivider DividerType="DividerType.Middle" Class="my-6"/>
        <MudText GutterBottom="true">Known Issues</MudText>
         <MudTreeView T="string" ReadOnly>
    <MudTreeViewItem T='string' Text="Slow Upload Speed">
        <MudAlert Severity="Severity.Normal" Dense="true" Class="my-2" Variant='Variant.Outlined'>Tracked for v2.0.4-dev</MudAlert>
        <MudTreeViewItem  T='string' Class="my-4" Text="Based on testing, 30 page files can take 1-1.5 minutes to upload, 90: 6 minutes" />
        <MudTreeViewItem  T='string' Class="my-4" Text="Currently being addressed by giving users a 10 minute timeout. " />
    </MudTreeViewItem>
    <MudTreeViewItem T='string' Text="Voice Chat Non-Functional">
        <MudAlert Severity="Severity.Info" Dense="true" Class="my-2" Variant='Variant.Outlined'>Feature will be removed in v2.0.3 release</MudAlert>
        <MudTreeViewItem T='string'  Class="my-4" Text="Voice chat is not supported in the chat view" />
        <MudTreeViewItem T='string'  Class="my-4" Text="We have no plans to continue support it in future updates and will remove the UI element" />
    </MudTreeViewItem>
         </MudTreeView>
        
                 
    </MudExpansionPanel>
            </MudPaper>
        </ItemContent>
    </MudTimelineItem>
</MudTimeline>


@code {
    bool _expanded = false;

    private void OnExpandCollapseClick() {
        _expanded = !_expanded;
    }
  
}

@code{
    private TimelineAlign _timelineAlign { get; set; } = TimelineAlign.Start;
  
}